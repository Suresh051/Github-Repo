pipeline {
    agent any

    stages {

        stage('checkout') {
          steps {
          checkout([$class: 'GitSCM', branches: [[name: '**/tags/**']], extensions: [], userRemoteConfigs: [[refspec: '+refs/tags/*’:’refs/remotes/origin/tags/*', url: 'https://github.com/Suresh051/Github-Repo.git']]])
          }
        
         }
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('Test') {
            steps {
                echo 'Testing..'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying....'
            }
        }
    }
}
