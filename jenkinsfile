pipeline {
agent {
dockerfile true
}

stages{
stage('Eample'){
steps {
echo 'Hello World!'
sh 'echo myCustomEnvVar=$myCustomEnvVar'
      }
     }
  def tag = sh(returnStdout: true, script: "git tag --contains | head -1").trim()
    if (tag) {
        stage("deploy") {
          steps {
            echo "Deloying to dev"
            }
        }
    }   
   }
 }
